{
    "name": "rusblaze/petrinet",
    "type": "library",
    "description": "Workflow module for Zend Framework with support of Doctrine2 entities",
    "keywords": ["petrinet", "workflow", "zf2", "queue", "doctrine"],
    "homepage": "https://github.com/rusblaze/doctrine2-zf2-petrinet-workflow-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Ivanov",
            "email": "rusblaze@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "2.*",
        "slm/queue-doctrine": "~0.5.0",
        "doctrine/doctrine-orm-module":  "~0.7"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "psr-4": {
            "Petrinet\\": "src/"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
